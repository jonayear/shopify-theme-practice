{% if pagination.pages > 0 %}
<nav aria-label="Page navigation example text-center m-auto">
  <ul class="pagination ">
  	{%  if pagination.previous %}
  		<li class="page-item"><a class="page-link" href="{{ pagination.previous.url }}">{{ pagination.previous.title }}</a></li>
  	{% endif %}
  	{%  if pagination.next %}
  		<li class="page-item"><a class="page-link" href="{{ pagination.next.url }}">{{ pagination.next.title }}</a></li>
  	{% endif %}
  </ul>
</nav>
{% endif %}